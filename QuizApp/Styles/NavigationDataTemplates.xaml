<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:QuizApp">
    <DataTemplate DataType="{x:Type local:BrowseFragmentVM}">
        <local:BrowseFragment />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:CourseCardsContainerVM}">
        <local:CourseCardsContainer />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:BrowseOverviewVM}">
        <local:BrowseOverviewFragment />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:CourseExpandedVM}">
        <local:CourseExpanded />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:CourseCardGridVM}">
        <local:CourseCardsGridFragment x:Name="grid"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:CategoriesCardContainerVM}">
        <local:CategoriesCardContainer />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:InstructorsCardContainerVM}">
        <local:InstructorsCardContainer />
    </DataTemplate>
    
</ResourceDictionary>